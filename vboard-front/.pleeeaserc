{
    "in": [
        "src/main/common/*/styles/*.css",
        "src/main/common/*/styles/*/*.css"
    ],
    "out": "app-pleeeased.css",
    "autoprefixer": {"browsers": ["last 2 versions", "> 2%", "Explorer >= 8", "Android >= 4.1"], "cascade": true},
    "filters": true,
    "rem": ["10px"],
    "pseudoElements": true,
    "opacity": true,
    "import": true,
    "minifier": false,
    "mqpacker": false,
    "sourcemaps": true,
    "next": true
}

