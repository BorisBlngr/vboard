{
    "plugins": ["angular"],
    "extends": ["strict", "plugin:angular/johnpapa"],
    "parserOptions": {
        "ecmaVersion": 8
    },
    "env": {
        "browser": true
    },
    "rules": {
        // Angular JS
        "angular/controller-as": 0,
        "angular/controller-as-route": 0,
        "angular/controller-as-vm": 0,
        "angular/file-name": 0,
        "angular/no-run-logic": 0,

        // Possible Errors
        "comma-dangle": 0,
        "no-cond-assign": [2, "always"],
        "no-console": 0,
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-dupe-keys": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": 0,
        "no-extra-semi": 2,
        "no-func-assign": 0,
        "no-inner-declarations": 0,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 0,
        "no-reserved-keys": 0,
        "no-sparse-arrays": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-jsdoc": 0,
        "valid-typeof": 2,

        // Best Practices
        "block-scoped-var": 0,
        "complexity": [2, 8],
        "consistent-return": 2,
        "curly": [2, "all"],
        "default-case": 0,
        "dot-notation": 0,
        "eqeqeq": [2],
        "guard-for-in": 0,
        "no-alert": 2,
        "no-caller": 0,
        "no-div-regex": 2,
        "no-else-return": 2,
        "no-labels": 2,
        "no-empty-function": 0,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 0,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-implied-eval": 2,
        "no-iterator": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-spaces": 0,
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-new": 0,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-process-env": 0,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-unused-expressions": 2,
        "no-void": 2,
        "no-warning-comments": 0,
        "no-with": 2,
        "prefer-arrow-callback": 0,
        "radix": 2,
        "vars-on-top": 0,
        "wrap-iife": [2, "outside"],
        "yoda": 0,

        // Strict Mode
        "no-extra-strict": 0,
        "global-strict": 0,
        "strict": [2, "global"],

        // Variables
        "no-catch-shadow": 0,
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-shadow": 2,
        "no-shadow-restricted-names": 2,
        "no-undef": 2,
        "no-undef-init": 2,
        "no-undefined": 2,
        "no-unused-vars": [2, {"vars": "local", "args": "after-used"}],
        "no-use-before-define": 2,

        // Stylistic Issues
        "array-bracket-newline": 0,
        "array-bracket-spacing": 0,
        "id-blacklist": 0,
        "id-length": [2, {"exceptions": ["$q"]}],
        "brace-style": [2, "1tbs", { "allowSingleLine": true }],
        "camelcase": 2,
        "comma-spacing": 2,
        "comma-style": [2, "last"],
        "consistent-this": 0,
        "eol-last": 0,
        "filenames/match-regex": 0,
        "func-names": 0,
        "func-style": 0,
        "indent": [2, 4],
        "key-spacing": 0,
        "max-nested-callbacks": [2, 3],
        "no-magic-numbers": 0,
        "new-cap": 2,
        "new-parens": 2,
        "no-array-constructor": 2,
        "no-inline-comments": 0,
        "no-lonely-if": 0,
        "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
        "no-multiple-empty-lines": [2, {"max": 2}],
        "no-nested-ternary": 2,
        "no-new-object": 2,
        "line-comment-position": 0,
        "no-spaced-func": 2,
        "no-ternary": 0,
        "no-trailing-spaces": 0,
        "no-underscore-dangle": 0,
        "one-var": 0,
        "operator-assignment": 0,
        "padded-blocks": 0,
        "quote-props": 0,
        "quotes": 0,
        "semi": 0,
        "semi-spacing": 2,
        "sort-vars": 0,
        "space-after-function-name": 0,
        "keyword-spacing": 2,
        "space-before-blocks": [2, "always"],
        "space-before-function-parentheses": 0,
        "space-in-brackets": 0,
        "space-in-parens": 0,
        "space-infix-ops": 0,
        "space-unary-ops": 2,
        "spaced-line-comment": 0,
        "wrap-regex": 2,

        // Legacy
        "max-depth": [2, 5],
        "max-len": 0,
        "max-params": 0,
        "max-statements": 0,
        "no-bitwise": 2,
        "no-plusplus": 0
    },
    "globals": {
        "_": false,
        "$": false,
        "API_ENDPOINT": false,
        "afterEach": false,
        "angular": false,
        "beforeEach": false,
        "browser": false,
        "describe": false,
        "expect": false,
        "imagesLoaded": false,
        "inject": false,
        "it": false,
        "MONTHS_RANGE_FETCHED": false,
        "module": false,
        "moment": false,
        "protractor": false,
        "Packery": false,
        "twttr": false
    }
}
